{
  "scripts": {
    "dev": "parcel src/editor/index.html",
    "build": "parcel build src/editor/index.html --public-url /web-tidal --dist-dir dist/web-tidal --no-scope-hoist",
    "clean-branches": "git fetch -p && for branch in $(git for-each-ref --format '%(refname) %(upstream:track)' refs/heads | awk '$2 == \"[gone]\" {sub(\"refs/heads/\", \"\", $1); print $1}'); do git branch -D $branch; done",
    "typecheck": "tsc --noEmit"
  },
  "type": "module",
  "devDependencies": {
    "@lezer/generator": "^1.7.0",
    "@parcel/transformer-inline-string": "^2.12.0",
    "parcel": "^2.12.0",
    "parcel-transformer-lezer": "^0.2.1",
    "process": "^0.11.10",
    "typescript": "^5.6.3"
  },
  "dependencies": {
    "@codemirror/language": "^6.0.1",
    "@codemirror/legacy-modes": "^6.0.1",
    "@codemirror/state": "^6.0.1",
    "@lezer/common": "^1.2.1",
    "@lezer/lr": "^1.4.1",
    "@management/cm-evaluate": "^0.1.0",
    "@strudel/core": "~1.0.1",
    "@strudel/mini": "~1.0.1",
    "@strudel/tonal": "~1.0.1",
    "@strudel/webaudio": "~1.0.1",
    "codemirror": "^6.0.1",
    "marked": "^14.1.2",
    "preact": "^10.24.2",
    "thememirror": "^2.0.1"
  },
  "alias": {
    "preact/jsx-dev-runtime": "preact/jsx-runtime"
  }
}
