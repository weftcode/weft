{
  "scripts": {
    "dev": "vite serve src/editor",
    "build": "parcel build src/editor/index.html --public-url ./ --dist-dir dist --no-scope-hoist",
    "clean-branches": "git fetch -p && for branch in $(git for-each-ref --format '%(refname) %(upstream:track)' refs/heads | awk '$2 == \"[gone]\" {sub(\"refs/heads/\", \"\", $1); print $1}'); do git branch -D $branch; done",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "test-update": "jest --updateSnapshot"
  },
  "type": "module",
  "workspaces": [
    "src/*"
  ],
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@lezer/generator": "^1.7.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "typescript": "^5.6.3",
    "vite": "^6.3.5"
  },
  "dependencies": {
    "@codemirror/language": "^6.0.1",
    "@codemirror/legacy-modes": "^6.0.1",
    "@codemirror/state": "^6.0.1",
    "@lezer/common": "^1.2.1",
    "@lezer/lr": "^1.4.1",
    "@management/cm-evaluate": "^0.2.0",
    "@strudel/core": "^1.2.2",
    "@strudel/mini": "^1.2.2",
    "@strudel/tonal": "^1.2.2",
    "@strudel/webaudio": "^1.2.3",
    "codemirror": "^6.0.1",
    "marked": "^14.1.2",
    "preact": "^10.24.2"
  },
  "alias": {
    "preact/jsx-dev-runtime": "preact/jsx-runtime"
  }
}
